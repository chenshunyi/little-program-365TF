/* page/checkprices/result.wxss */
.searchresult{display: block;}
.houseinf{width: 96%;float:left;position: relative;margin: 0 3%;border-bottom:0.5rpx solid #d9d9d9; }
.houseinf_name{float: left;height: 80rpx;line-height: 80rpx;margin-left: 10rpx;font-size: 30rpx;}
.hosueinf_size{float:left;height: 80rpx;padding-left: 10px;line-height: 80rpx;font-size: 30rpx;}
.houseshu{width: 10rpx;height: 40rpx;background: #ff7500;position: absolute;left: -3%;z-index: 22;top: 20rpx;}

.clearf{clear: both;}
.totalprice,.danjia{
    width: 50%;
    float: left;
    height: 100rpx;
    margin-top: 20rpx;
    margin-bottom: 20rpx;
    text-indent: 40rpx;
    }
.totalprice{border-right: 1px solid #cccccc;box-sizing: border-box;-webkit-border-box:border-box;}

.money,.moneyNum{color: #666666;font-size: 26rpx;line-height: 50rpx;display: block;}
.moneyNum{color: #303030;font-size: 36rpx;font-weight: bold;}

.pricetrend{width: 94%;display:block;border-bottom: 0.5rpx solid #d9d9d9;height: 60rpx;line-height: 60rpx;color: #4d4d4d;font-size: 24rpx;background: url(data:image/png;base64,) no-repeat 3% center;
background-size: 32rpx 26rpx;
padding: 0 3%;
text-indent: 5%;
}

.averageprice,.contrast{font-size: 36rpx;color: #303030;font-weight: bold;line-height: 60rpx;}
.square{font-size: 24rpx;font-weight: normal;}

.contrast{color: #41a200;display: block;font-weight: bold;}
.contrast-up{color: #ff0030;display: block;font-weight: bold;}
.contrast-o{color: #EEEEEE;display: block;font-weight: bold;}

.pricetrend.secondtj{background: url(data:image/png;base64,) no-repeat 3% center;
background-size: 27rpx 27rpx;
}

.trendchart{
    width: 100%;
    padding:15rpx 0;
    border-top: 0.5rpx solid #d9d9d9;
    text-align: center;
    float: left;}

    .dot{
        width:18rpx;
        height:18rpx;
        background: #ff7500;
        border-radius: 18rpx;
        -webkit-border-radius:18rpx;
        float: left;
        margin-top: 16rpx;
        }
    .currentfield{float: left;font-size: 24rpx;color: #ff7500;font-weight: bold;line-height: 50rpx;padding-left: 10rpx;}
    .currentunit{float: right;font-size: 24rpx;color: #303030;line-height: 50rpx;}
    #firstCanvas {margin:0 auto;}

    