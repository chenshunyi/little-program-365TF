<form bindsubmit="formSubmit">
<navigator url="area" open-type="navigate">
    <view class="searchprice_con">
        <text class="searchprices_name">小区名称</text>        
        <text class="searchprices_select">{{blockname}}</text>
        <image class="searchprices_img" src="data:image/png;base64,"></image>
    </view>
</navigator>
<view class="searchprice_con">
    <text class="searchprices_name">面积</text>
    <view class="searchprices_select"><input class="searchprices_size" type="digit" name="area" placeholder-class="placeholder-class" placeholder="请输入面积" maxlength="6" /></view>
    <text class="searchprices_unit">m²</text>
</view>
<button class="searchpriceBtn marginTop" formType="submit">快速评估</button>
</form>