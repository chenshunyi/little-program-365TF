.flex--sa {
    -moz-box-pack: justify; 
    -webkit-box-pack: justify;
    box-pack: justify;
    
    justify-content: space-around;
    -moz-justify-content: space-around;
    -ms-justify-content: space-around;
    -o-justify-content: space-around;
}
page{
	overflow: hidden;
}
/* 蒙层 */
.winPop{
    position: absolute;
    left: 0;
    top: 0;
    
    height: 100%;
    width: 100%;
    
    background: rgba(0,0,0,.75);
    
    z-index: 998;
}
.headSearch{
	position: relative;
	z-index: 999;

	height: 82rpx;
	border-bottom: 1rpx solid #ddd;
  -webkit-box-shadow: 0 3rpx 7rpx rgba(0,0,0,.1);
	box-shadow: 0 3rpx 7rpx rgba(0,0,0,.1);
	overflow:hidden;
	
	background-color: #fff;
	text-indent: 30rpx;
	line-height: 82rpx;
  text-align: center;
	color: #ff7500;
	font-size: 30rpx;
}
.selectDown{
	position: relative;
	z-index: 99;
  left: 10rpx;
  top: -5rpx;
  width: 17rpx;
  height: 11rpx;
  background: no-repeat  url(data:image/png;base64,);
  background-size: 100%;
}
/* 筛选栏 */
.headSelect{
	position: relative;
	z-index: 999;

	height: 77rpx;
	border-bottom: 1rpx solid #d9d9d9;

	background:#fff;
}
.headSearch-l{
	width: 100%;
	height: 82rpx;
}
.icon-car{
  position: relative;
  right: 10rpx;
  top: 6rpx;
  width: 35rpx;
  height: 36rpx;
   background: no-repeat url(data:image/png;base64,);
  background-size: 100%;
}
.headSelect__item{
	height: 77rpx;
	width: 95rpx;

	background-image:url(data:image/png;base64,);
	background-repeat: no-repeat;
	background-position: right center;
	-webkit-background-size: 19rpx 12rpx;
	background-size: 19rpx 12rpx;

	font-size: 30rpx;
	line-height: 77rpx;
}
.headSelect__item.on{
	color: #ff7500;
}
/* 下拉 */
.headSelect__downlist{
	position: absolute;
	top: 84rpx;
	left: 0;
	z-index: 999;
	width: 100%;
	background: #fff;
}
.headSelect__downlistItem{
  width: 700rpx;
	height: 87rpx;
	margin: 0 auto;
	line-height: 87rpx;
	text-align: center;
}
.headSelect__downlistItem.bottomLine{
	border-bottom: 1rpx solid #d9d9d9;
}
/*  */
.list__contain{
	padding: 16rpx 36rpx;
	/*background: #eeeeee;*/
	color: #303030;
}
.list__item{
	overflow:hidden;
	border-top: 1rpx solid  #d9d9d9;
	padding: 0 30rpx;
	background: #fff;
}
.list__title{
	box-sizing: border-box;
	padding: 29rpx 0;

	background-color: #fff;
	font-size: 32rpx;
}
.list__title wx-text {
	border-left:8rpx solid #ff7500;
	padding:5rpx 0 5rpx 20rpx;
}
.item__time{
	margin: 20rpx 0 0 0;
	background: url(data:image/png;base64,) no-repeat left center;
	-webkit-background-size: 22rpx 23rpx;
	background-size: 22rpx 23rpx;
	text-indent: 36rpx;
}
.item__addr{
  position: relative;
  margin-top: 10rpx;
  line-height: 40rpx;
	text-indent: 36rpx;
}
.addricon{
  position: absolute;
  left: 0;
  top: 10rpx;
  width: 18rpx;
  height: 24rpx;
  background: url(data:image/png;base64,)  no-repeat;
  background-size: 100%;
}
.item__tags{
	margin: 0 0 26rpx 0;
}
.item__tag{
	float: left;
	/*border-radius: 4rpx;
	border:1rpx solid #666666;*/
	margin: 0 10rpx 0 0;
	padding: 2prx 3rpx;
	font-size: 22rpx;
	color: #666666;
}
.map{
	width: 100%;
	height: 210rpx;
	background-repeat: no-repeat;
	background-size: 100%;
	-webkit-background-size: 100%;
}
.map__list{
	width: 100%;
	height: 81rpx;
	border-bottom: 1rpx solid #dddddd;
}
.map__name{
	width: 246rpx;
	height: 81rpx;
	overflow: hidden;
	line-height: 81rpx;
	font-size: 30rpx;
}
.map__name text{
	text-overflow: ellipsis;
}
.map__tag{
	width: 220rpx;
	height: 81rpx;

	color: #f1553e;
	line-height: 81rpx;
}
.map__tag text{
	padding: 2rpx 6rpx;
	border-radius: 4rpx;
	border:1rpx solid #f1553e;
	text-overflow: ellipsis;
}
.map__price{
	float: right;
	width: 150rpx;
	height: 81rpx;
	color: #ff7500;
	line-height: 81rpx;
}
.tel-btn,.enroll-btn{
	width: 199rpx;
	height: 59rpx;
	margin: 44rpx 0rpx;
	-webkit-border-radius: 4rpx 5rpx 5rpx 4rpx/5rpx;
	-moz-border-radius: 4rpx 5rpx 5rpx 4rpx/5rpx;
	border-radius: 4rpx 5rpx 5rpx 4rpx/5rpx;
	background-color: #448bc3;
	border: solid 1rpx #448bc3;
	line-height: 59rpx;
	text-align: center;
	color: #fff;
	font-size: 30rpx;
}
.tel-btn{
	margin-right: 76rpx;
}
.enroll-btn{
	margin-left: 76rpx;
	
	background-color: #f50;
	border: solid 1rpx #f50;
}