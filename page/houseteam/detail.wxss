/* 底部 */
.tel-fixed{
    width: 100%;
    height: 110rpx;
}
.tel-content{
    height: 110rpx;
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    z-index: 999;
}
.tel-detail{
    background-color: rgba(0, 0, 0, .8);
    width: 70%;
    padding: 20rpx 5%
}
.tel-info{color: #8a8a8a;margin-top: 10rpx;}
.tel{width: 30%;}
.icon-tel{
    background: url(data:image/png;base64,) no-repeat center;
    background-size: 53rpx 49rpx;
    width: 53rpx;
    height: 49rpx;
}
/**/
.map{
	width: 693rpx;
	height: 316rpx;
	margin: 28rpx auto 0;
	transition:transform .4s;
}
.map.mapDown{
	margin: 0;
	width: 100%;
}
/*  */
.list__contain{
    padding: 16rpx 36rpx;
    background: #eeeeee;
    color: #303030;
}
.list__item{
    overflow:hidden;
    border-top: 1rpx solid  #d9d9d9;
    padding: 0 30rpx;
    background: #fff;
    color: #303030;
}
.list__title{
    box-sizing: border-box;
    padding: 29rpx 0;
    border-bottom:1rpx solid #ddd;

    background-color: #fff;
    font-size: 32rpx;
    font-weight: bold;
}
.list__title-r{
    float: right;
    width: 340rpx;
    overflow: hidden;
    font-size: 26rpx;
    text-align: right;
    font-weight: normal;
}
.item__time{
    margin: 20rpx 0 0 0;
    background: url(data:image/png;base64,) no-repeat left center;
    -webkit-background-size: 22rpx 23rpx;
    background-size: 22rpx 23rpx;
    text-indent: 36rpx;
}
.item__addr{
    background: url(data:image/png;base64,) left center no-repeat;
    -webkit-background-size: 18rpx 23rpx;
    background-size: 18rpx 23rpx;
    line-height: 76rpx;
    text-indent: 36rpx;
}
.item__tags{
    padding: 0 0 26rpx 0;
    border-bottom: 1rpx solid #ddd;
}
.item__tag{
    float: left;
    border-radius: 4rpx;
    /*border:1rpx solid #666666;*/
    margin: 0 10rpx 0 0;
   	padding: 2rpx 3rpx;
    font-size: 22rpx;
    color: #666666;
}
.item__gift,.item__other{
    max-height: 130rpx;
    height: auto;
    box-sizing: border-box;
    padding-bottom: 20rpx;
    border-bottom: 1rpx solid #ddd;
}
.item__gift-l,.item__other-l{
    width: 196rpx;
    margin: 29rpx 0 0 0;
    font-size: 26rpx;
    background: left center no-repeat url(data:image/png;base64,);
    -webkit-background-size: 26rpx;
    background-size: 26rpx;
   text-indent: 50rpx; 
}
.item__other-l{
    background: left center no-repeat url(data:image/png;base64,);
    -webkit-background-size: 28rpx 23rpx;
    background-size: 28rpx 23rpx;
}
.item__gift-r,.item__other-r{
	padding: 16rpx 0 0 0;
	box-sizing: border-box;
    width: 480rpx;
}
.item__tag2{
    float: left;
    width: 138rpx;
    height: 36rpx;
    margin: 11rpx 0 0 22rpx;
    
    background-repeat: no-repeat;
    background-position: left center;
	
	text-indent: 40rpx;
	color: #666666;
    font-size: 24rpx;
    line-height: 36rpx;
}
