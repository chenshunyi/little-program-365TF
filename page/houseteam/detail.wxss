.list__item.line{
	padding: 0;
	margin: 0 30rpx;
}
.list__title.line{
	font-size: 24rpx;
	color: #4d4d4d;
	font-weight: normal;
	border-bottom: none;
}
.list__title.line wx-text {
	border-left:8rpx solid #ff7500;
	padding:5rpx 0 5rpx 20rpx;
}
.context{
	margin: 24rpx 0;
	font-size: 30rpx;
	line-height: 44rpx;
    border-bottom: 1rpx solid  #d9d9d9;
}
.activity,.declare{
	position: relative;
	height: 84rpx;
    border-bottom: 1rpx solid  #d9d9d9;
    line-height: 84rpx;
	font-size: 30rpx;
}
.or-btn{
	position: absolute;
	top: 28rpx;
	right: 0;
	width: 16rpx;
	height: 25rpx;
	background: no-repeat url(data:image/png;base64,);
	background-size: 100%;
	-webkit-background-size: 100%;
}
