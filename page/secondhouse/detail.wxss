/* page/secondhouse/index.wxss */
.swiperList{
	width: 100%;
	height: 403rpx;
	margin: 0 auto;
}
.swiper-item{
	background: #cfcfcf;
	text-align: center;
}
.slide-image{
	height: 100%;
	width: 100%;
}
/*  */
.headTitle{
	padding: 26rpx 20rpx;
	border-bottom: 1rpx solid #d9d9d9;
}
.mainTitle{
	display: inline-block;

	font-size: 32rpx;
	line-height: 48rpx;
	color: #303030;
}
.headTitle__tag{
	display: inline-block;
	padding: 2rpx 16rpx;
	font-size: 24rpx;
	line-height: 30rpx;
	color: #fff;
	background: #ff5500;	
}
.select__tagList{
	margin: 26rpx 0 0 0;
}
.select__tag{
	margin: 0 6rpx;
	padding: 1rpx 10rpx;
	background-color: #fff;
	color: #619cdd;
	font-size: 24rpx;
}
/*  */
.schoolArea{
	width: 700rpx;
	height: 78rpx;
	padding: 24rpx 0;
	box-sizing: border-box;
	margin: 0 auto;
}
.bd{
	border-bottom: dashed 1rpx #d9d9d9;
}
.schoolArea__tag{
	position: relative;
	float: left;
	padding: 1rpx 2rpx;
	font-size: 24rpx;
	color: #fff;
	background: #ff5500;
}
.schoolArea__tri{
	position: absolute;
	right: -6rpx;
	top: 0;
	width: 7rpx;
	height: 30rpx;

	background: right center no-repeat url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAcAAAAeBAMAAAASpRmzAAAAJ1BMVEUAAAD/VQD/VQD/VQD/VQD/VQD/VQD/VQD/VQD/VQD/VQD/VQD/VQCZaH0lAAAADHRSTlMAzqd3aD0eCurpS0lpooE7AAAAKUlEQVQI12MgCSwAEYdBxBl1EHEwAUicaQURZxxgxFSYhDpM8QYGEgEAXkESKRqK7AoAAAAASUVORK5CYII=);
	-webkit-background-size: 100%;
	background-size: 100%;
}
.schoolArea__txt{
	float: left;
	text-indent: 20rpx;
	font-size: 26rpx;
}
/*  */
.secondhouse__price{
	height: 98rpx;
	border-bottom: solid 1rpx #d9d9d9;
	
	font-size:28rpx;
	color: #666666;
	line-height:98rpx;
	background-color:#f2f2f2;
	text-indent:28rpx;
}
.price{
	font-size: 36rpx;
	color: #ff5500;
}
.secondhouse__squareL,.secondhouse__squareR{
	width: 50%;
	height: 80rpx;
	background-color:#f2f2f2;

	line-height: 80rpx;
	text-indent:28rpx;
	font-size: 24rpx;
	color: #666666;
}
.squareL,.squareR{
	color: #404040;
	font-size: 28rpx;
}
/*  */
.dynamic{
    font-size: 28rpx;
    line-height: 40rpx;
}
.detail-tit{
	position: relative;
    padding-top: 20rpx;
    padding-bottom: 20rpx;
}
.detail-tit text{
    border-left: 8rpx solid #ff7500;
    padding: 5rpx 0 5rpx 20rpx;
}
.house__detailList{
	font-size: 28rpx;
	color: #303030;
}
.house__detailListL,.house__detailListR{
	text-indent:28rpx;

	margin: 23rpx 0 0 0;
	width: 50%;
}
.publicTime{
	height: 80rpx;
	color: #303030;
	text-indent: 28rpx;
	line-height: 80rpx;
}
.btn-down{
	width: 100%;
	height: 46rpx;
	margin: -10rpx 0 0 0;
	background: center center no-repeat url(data:image/png;base64,);
	-webkit-background-size: 34rpx 20rpx;
	background-size: 34rpx 20rpx;
}
.btn-down.up{
	transition:transform .4s;
	transform:rotate(180deg);
}
.detail-tri{
	position: absolute;
	right: 20rpx;
	top: 20rpx;
	width: 14rpx;
	height: 23rpx;

	background: no-repeat url(data:image/png;base64,);
	-webkit-background-size: 100%;
	background-size: 100%;
}
.lh60{
	line-height: 50rpx;
}
#map{
	width: 705rpx;
	margin: 26rpx auto 0;
}
.mappic{
	width: 100%;
	height: 260rpx;
}
.tel-fixed{
    width: 100%;
    height: 110rpx;
}
.tel-content{
    height: 110rpx;
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
	z-index: 999;
}
.tel-detail{
    background-color: rgba(0, 0, 0, .8);
    width: 70%;
    padding: 20rpx 5%
}
.tel-info{color: #8a8a8a;margin-top: 10rpx;}
.tel{width: 30%;}
.icon-tel{
    background: url(data:image/png;base64,) no-repeat center;
    background-size: 53rpx 49rpx;
    width: 53rpx;
    height: 49rpx;
}
.textHide{
	height: 160rpx;
	overflow-y: hidden;

	text-overflow: ellipsis;
	word-break: break-all;
}
.house-addr{
	margin:20rpx 28rpx;
	color: #303030;
}
.house-addr text{
	color: #ff7500;
}