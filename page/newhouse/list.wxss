.flex--sa {
    -moz-box-pack: justify; 
    -webkit-box-pack: justify;
    box-pack: justify;
    
    justify-content: space-around;
    -moz-justify-content: space-around;
    -ms-justify-content: space-around;
    -o-justify-content: space-around;
}
/* 蒙层 */
.winPop{
    position: absolute;
    left: 0;
    top: 0;
    
    height: 100%;
    width: 100%;
    
    background: rgba(0,0,0,.75);
    
    z-index: 998;
}
.headSearch{
	position: relative;
	z-index: 999;

	height: 88rpx;
	overflow:hidden;

	background-color: #f2f2f2;
}
.headSearch__inner{
	position: relative;
	width: 706rpx;
	height: 56rpx;
	margin: 16rpx auto;

	-webkit-border-radius: 10rpx;
	-moz-border-radius: 10rpx;
	border-radius: 10rpx;
	background-color: #fff;
	border: solid 1rpx #e6e6ea;
	
	line-height: 56rpx;
	color: #cccccc;
	text-indent: 53rpx;
}
.icon-search{
	position: absolute;
	top: 13rpx;
	left: -40rpx;
	z-index: 99;
}
/* 筛选栏 */
.headSelect{
	position: relative;
	z-index: 999;

	height: 77rpx;
	border-bottom: 1rpx solid #d9d9d9;

	background:#fff;
}
.headSelect__item{
	height: 77rpx;
	width: 95rpx;

	background-image:url(data:image/png;base64,);
	background-repeat: no-repeat;
	background-position: right center;
	-webkit-background-size: 19rpx 12rpx;
	background-size: 19rpx 12rpx;

	font-size: 30rpx;
	line-height: 77rpx;
}
.headSelect__item.on{
	color: #ff7500;
}
/* 下拉 */
.headSelect__downlist{
	position: absolute;
	top: 78rpx;
	left: 0;
	z-index: 999;
	width: 100%;
	background: #fff;
}
.headSelect__downlistItem{
    width: 700rpx;
	height: 87rpx;
	margin: 0 auto;
	line-height: 87rpx;
	text-align: center;
}
.headSelect__downlistItem.on{
	color: #ff7500;
}
.headSelect__downlistItem.bottomLine{
	border-bottom: 1rpx solid #d9d9d9;
}