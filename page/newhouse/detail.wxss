.swiperList{
	margin: 0 auto;
	width: 100%;
	height: 560rpx;
    background-color: #cfcfcf;
    text-align:center;
}
.slide-image{
	max-width: 100%;
	height: 560rpx;
    margin: 0 auto;
    width: 100%;
}
.detail-tit{
    padding-top: 20rpx;
    padding-bottom: 20rpx;
}
.detail-tit text{
    border-left: 8rpx solid #ff7500;
    padding: 5rpx 0 5rpx 20rpx;
}
.dynamic{
    font-size: 28rpx;
    line-height: 40rpx;
}
.map{
    width: 100%;
    height: 260rpx;
}
.tel-fixed{
    width: 100%;
    height: 110rpx;
}
.tel-content{
    height: 110rpx;
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    z-index: 999;
}
.tel-detail{
    background-color: rgba(0, 0, 0, .8);
    width: 70%;
    padding: 20rpx 5%
}
.tel-info{color: #8a8a8a;margin-top: 10rpx;}
.tel{width: 30%;}
.icon-tel{
    background: url(data:image/png;base64,) no-repeat center;
    background-size: 53rpx 49rpx;
    width: 53rpx;
    height: 49rpx;
}
.house-type{
    width: 94%;
    padding: 0 3%;
    height: 290rpx;
    overflow: hidden;
    white-space: nowrap;
}
.a-type{
    display: inline-block;
    padding: 25rpx 10rpx;
}
.a-type image{
    width: 200rpx;
    border: 1px solid #d9d9d9;
    height: 196rpx;
}
.icon-time,.icon-address{
    position: relative;
    top: 4rpx;
}
.icon-time{
    background: url(data:image/png;base64,) no-repeat center;
    background-size: 26rpx 26rpx;
    width: 26rpx;
    height: 26rpx;
}
.icon-address{
    background: url(data:image/png;base64,) no-repeat center;
    background-size: 23rpx 31rpx;
    width: 23rpx;
    height: 31rpx;
}
.avg-price{
    color: #ccc;
}
#firstCanvas{
    margin: 0 auto;
}
.mappic{
	width: 100%;
	height: 260rpx;
}